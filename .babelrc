{
    "presets": [
        ["@babel/preset-env", { "loose": true }],
        "@babel/typescript",
        "@babel/react"
    ],
    "plugins": [
        ["@babel/plugin-proposal-class-properties", { "loose": true }],
        "@babel/proposal-object-rest-spread",
        "babel-plugin-styled-components",
        "@babel/plugin-proposal-optional-chaining"
    ]
}
